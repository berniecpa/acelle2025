[program:app-master]
process_name=%(program_name)s_%(process_num)02d
command={{ php_artisan }} queue:work --queue=import,default --tries=1 --max-time=180
autostart=true
autorestart=true
user=www-data
numprocs=2
redirect_stderr=true
stdout_logfile=/var/log/supervisor/master-output
stderr_logfile=/var/log/supervisor/master-error
